{{#each securityDefinitions}}
### {{@key}}
{{#this}}
{{#ifeq type "oauth2"}}
<table class="confluenceTable">
<colgroup><col><col></colgroup>
<tbody><tr>
        <th class="confluenceTh">type</th>
        <th class="confluenceTh" colspan="2">{{type}}</th>
    </tr>
{{#if description}}
        <tr>
            <th class="confluenceTh">description</th>
            <th class="confluenceTh" colspan="2">{{description}}</th>
        </tr>
{{/if}}
{{#if authorizationUrl}}
        <tr>
            <th class="confluenceTh">authorizationUrl</th>
            <th class="confluenceTh" colspan="2">{{authorizationUrl}}</th>
        </tr>
{{/if}}
{{#if flow}}
        <tr>
            <th class="confluenceTh">flow</th>
            <th class="confluenceTh" colspan="2">{{flow}}</th>
        </tr>
{{/if}}
{{#if tokenUrl}}
        <tr>
            <th class="confluenceTh">tokenUrl</th>
            <th class="confluenceTh" colspan="2">{{tokenUrl}}</th>
        </tr>
{{/if}}
{{#if scopes}}
    <tr>
        <td class="confluenceTh" rowspan="3">scopes</td>
{{#each scopes}}
            <td class="confluenceTd">{{@key}}</td>
            <td class="confluenceTd">{{this}}</td>
        </tr>
        <tr>
{{/each}}
    </tr>
{{/if}}
</tr></tbody></table>
{{/ifeq}}
{{#ifeq type "apiKey"}}
<table class="confluenceTable">
<colgroup><col><col></colgroup>
<tbody><tr>
        <th class="confluenceTh">type</th>
        <th class="confluenceTh" colspan="2">{{type}}</th>
    </tr>
{{#if description}}
        <tr>
            <th class="confluenceTh">description</th>
            <th class="confluenceTh" colspan="2">{{description}}</th>
        </tr>
{{/if}}
{{#if name}}
        <tr>
            <th class="confluenceTh">name</th>
            <th class="confluenceTh" colspan="2">{{name}}</th>
        </tr>
{{/if}}
{{#if in}}
        <tr>
            <th class="confluenceTh">in</th>
            <th class="confluenceTh" colspan="2">{{in}}</th>
        </tr>
{{/if}}
</tr></tbody></table>
{{/ifeq}}
{{#ifeq type "basic"}}
<table class="confluenceTable">
<colgroup><col><col></colgroup>
<tbody><tr>
        <th class="confluenceTh">type</th>
        <th class="confluenceTh" colspan="2">{{type}}</th>
    </tr>
{{#if description}}
        <tr>
            <th class="confluenceTh">description</th>
            <th class="confluenceTh" colspan="2">{{description}}</th>
        </tr>
{{/if}}
</tr></tbody></table>
{{/ifeq}}
{{/this}}
{{/each}}
